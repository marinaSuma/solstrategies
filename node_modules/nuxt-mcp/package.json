{
  "name": "nuxt-mcp",
  "type": "module",
  "version": "0.2.4",
  "description": "MCP server helping models to understand your Nuxt app better.",
  "license": "MIT",
  "homepage": "https://github.com/antfu/nuxt-mcp#readme",
  "repository": "antfu/nuxt-mcp",
  "bugs": "https://github.com/antfu/nuxt-mcp/issues",
  "keywords": [
    "nuxt",
    "mcp",
    "modelcontextprotocol"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/module.mjs"
  },
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "nitropack": "^2 || ^3",
    "nuxi": ">=3",
    "nuxt": ">=3.5.0",
    "vite": ">=6"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@nuxt/kit": "^4.0.2",
    "ansis": "^4.1.0",
    "async-cache-dedupe": "^2.2.0",
    "citty": "^0.1.6",
    "debug": "^4.4.1",
    "pathe": "^2.0.3",
    "unimport": "^5.2.0",
    "zod": "^3.25.76",
    "vite-plugin-mcp": "0.2.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@types/debug": "^4.1.12",
    "@types/node": "latest",
    "eslint": "^9.32.0",
    "nitropack": "^2.12.4",
    "nuxi": "^3.27.0",
    "nuxt": "^4.0.2",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "play": "DEBUG=nuxt:mcp:server,vite:mcp:server PORT=4000 nuxi dev playground",
    "play:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground"
  }
}